[nginx]
${nginx_public_ip} ansible_user=${nginx_user} ansible_ssh_private_key_file=../infra/keys/id_rsa_nginx

[consul]
${consul_private_ip} ansible_user=${consul_user} ansible_ssh_private_key_file=../infra/keys/id_rsa_consul ansible_ssh_common_args='-o ProxyCommand="ssh -i ../infra/keys/id_rsa_nginx -W %h:%p ${nginx_user}@${nginx_public_ip}"'
